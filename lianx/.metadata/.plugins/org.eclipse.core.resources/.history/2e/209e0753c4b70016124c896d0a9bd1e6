
public class Xian extends Thread{
//其中线程就是一个主线程
	public static void main(String[] args) {
		
	}
	public void run() {
			// TODO Auto-generated method stub
			super.run();
			
		}
}
class Cheng extends Thread{
	public void run() {
		// TODO Auto-generated method stub
		super.run();
		for (int i = 0; i < 20; i++) {
			System.out.println(i);
		}
	}
}
